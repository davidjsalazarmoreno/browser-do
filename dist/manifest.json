{
  "manifest_version": 2,
  "name": "BrowserDo",
  "options_page": "options.html",
  "version": "0.0.1",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
 "browser_action": {
    "default_icon": "assets/icon.png",
    "default_title": "Browser Do Settings",
    "default_popup": "options-browser-action.html"
  },
  "background": {
    "page": "background.html"
  },
  "commands": {
    "toggle-browser-do": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Toggle Browser Do"
    }
  },
  "permissions": [
    "activeTab",
    "pageCapture",
    "tabs",
    "bookmarks"
  ]
}
